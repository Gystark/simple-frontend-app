{
  "name": "src",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cypress/code-coverage": "^1.11.1",
    "@cypress/instrument-cra": "^1.0.0",
    "cypress": "^4.0.1",
    "eslint-plugin-cypress": "^2.9.0",
    "istanbul-lib-coverage": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.1"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-summary --reporter=lcov",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "require-coverage": "nyc report --check-coverage --branches=80 --functions=80 --lines=80 --statement=80",
    "start": "BROWSER=none react-scripts start",
    "start:test": "BROWSER=none react-scripts -r @cypress/instrument-cra start",
    "test": "npm-run-all -p -r start:test cy:run",
    "test:coverage": "npm-run-all test coverage require-coverage",
    "test:open": "npm-run-all -p -r start:test cy:open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
